- if @current_user
  .m20
    .fl.p10
      = link_to 'All subscriptions', subscriptions_path, {:class => "menu p5"}
    .fl.p10
      = link_to 'Back to Doctor', @doctor, {:class => "menu p5"}
  .clear10

- if @doctor && @doctor.subscription
  %h3 Subscription changed for #{@doctor.full_name}
- else
  .error
    Error: no subscription found for #{params[:email]}



